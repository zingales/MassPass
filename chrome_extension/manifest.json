{
	"name" : "MassPass",
	"version" : "0.1",
	"manifest_version": 2,
	"description": "Secure Password Administrator",
	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	},
  "background": {
    "scripts": ["background.js"]
  },
	"permissions": [
		"http://*/*",
		"https://*/*",
		"tabs",
		"clipboardRead", 
		"clipboardWrite"
	]
}
